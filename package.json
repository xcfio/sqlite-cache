{
    "name": "sqlite-cache",
    "version": "0.0.2",
    "description": "A simple SQLite cache for Node.js applications",
    "author": "xcfio",
    "homepage": "https://github.com/xcfio/sqlite-cache#readme",
    "main": "out/index.js",
    "license": "MIT",
    "scripts": {
        "fmt": "prettier --config=.prettierrc --write src/",
        "lint": "prettier --config=.prettierrc --check src/",
        "build": "(node --eval \"require('node:fs').rmSync('out', { recursive: true, force: true })\" || true) && tsc",
        "dev": "tsc --watch",
        "test": "(tsc --noEmit) && (vitest run)",
        "test:run": "vitest run",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui"
    },
    "keywords": [
        "cache",
        "sqlite"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/xcfio/sqlite-cache"
    },
    "bugs": {
        "url": "https://github.com/xcfio/sqlite-cache/issues",
        "email": "omarfaruksnx@gmail.com"
    },
    "funding": {
        "type": "patreon",
        "url": "https://www.patreon.com/xcfio"
    },
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "dependencies": {
        "@sinclair/typebox": "^0.34.41",
        "colorette": "^2.0.20",
        "fast-json-stringify": "^6.0.1"
    },
    "devDependencies": {
        "@types/node": "^24.0.10",
        "@vitest/ui": "4.0.2",
        "vitest": "^3.2.4"
    }
}
